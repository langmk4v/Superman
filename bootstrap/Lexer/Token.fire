import SourceFile;

namespace firec {
    enum TokenKind {
        Emtpy,
        Integer(int),
        Float(float),
        Char(char),
        String(string),
        Identifier,
        Punctuator(TokenPunctuators),
        Eof
    }

    enum TokenPunctuators {
        Plus,
        Minus
    }

    class Token {
        var kind: TokenKind;
        var text: string;
        var location: SourceLocation;
    }
}